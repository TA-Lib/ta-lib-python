import json

# Provided data
data = '''
{
    "balance": 99900000,
    "hold": 101829,
    "feesPaid": 133721
}
'''

# Convert data to JSON
financial_data = json.loads(data)

# Display the data
print(f"Balance: {financial_data['balance']} satoshis")
print(f"Hold: {financial_data['hold']} satoshis")
print(f"Fees Paid: {financial_data['feesPaid']} satoshis")

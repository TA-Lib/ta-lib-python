CFLAGS	=	-zq -w2 -ox -d=TA_SINGLE_THREAD -d=WIN32
CXXFLAGS=	-zq -w2 -ox -d=TA_SINGLE_THREAD -d=WIN32
INCPATH	=	-i="..\..\..\..\..\src\ta_common" -i="..\..\..\..\..\include"
LIB	=	wlib -b -c -n -q -p=512
MOC	=	moc


	$(LIB) $(TARGET) @$(TMPLIST)
	del $(TMPLIST)

moc: $(SRCMOC)

tmake: ../csr/win32/watcom/ta_common/Makefile

../csr/win32/watcom/ta_common/Makefile: ta_common/ta_common.pro

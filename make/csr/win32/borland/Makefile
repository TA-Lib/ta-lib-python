MAKEFILE=	Makefile
TMAKE	=	tmake

SUBDIRS =	ta_common \
		ta_func \
		ta_abstract \
		ta_libc \
		gen_code \
		ta_regtest

all: $(SUBDIRS)

ta_common:
	cd ta_common
	$(MAKE)
	@cd ..

ta_func:
	cd ta_func
	$(MAKE)
	@cd ..

ta_abstract:
	cd ta_abstract
	$(MAKE)
	@cd ..

ta_libc:
	cd ta_libc
	$(MAKE)
	@cd ..

gen_code:
	cd gen_code
	$(MAKE)
	@cd ..

ta_regtest:
	cd ta_regtest
	$(MAKE)
	@cd ..


tmake: ../csr/win32/borland/Makefile

../csr/win32/borland/Makefile: rootmake.pro
	tmake rootmake.pro -o ../csr/win32/borland/Makefile

tmake_all:
	cd ta_common
	$(TMAKE) ta_common.pro -o $(MAKEFILE)
	@cd ..
	cd ta_func
	$(TMAKE) ta_func.pro -o $(MAKEFILE)
	@cd ..
	cd ta_abstract
	$(TMAKE) ta_abstract.pro -o $(MAKEFILE)
	@cd ..
	cd ta_libc
	$(TMAKE) ta_libc.pro -o $(MAKEFILE)
	@cd ..
	cd gen_code
	$(TMAKE) gen_code.pro -o $(MAKEFILE)
	@cd ..
	cd ta_regtest
	$(TMAKE) ta_regtest.pro -o $(MAKEFILE)
	@cd ..


clean:
